{
  "name": "ts-angularjs-webpack-starter",
  "version": "0.0.1",
  "private": true,
  "main": "src/server/main.ts",
  "scripts": {
    "nodemon": "nodemon --exec 'ts-node --inspect .' --ext 'ts' --watch 'src/server'",
    "start": "concurrently \"npm run webpack\" \"npm run nodemon\"",
    "webpack": "webpack-dev-server -d --content-base public --open --port 9001"
  },
  "dependencies": {
    "@angular/common": "~5.0.0",
    "@angular/compiler": "~5.0.0",
    "@angular/core": "~5.0.0",
    "@angular/platform-browser": "~5.0.0",
    "@angular/platform-browser-dynamic": "~5.0.0",
    "@angular/router": "~5.0.0",
    "bootstrap-css-only": "~4.0.0-beta",
    "express": "~4.16.0",
    "pg": "~7.4.0",
    "rxjs": "~5.5.0",
    "zone.js": "~0.8.0"
  },
  "devDependencies": {
    "@types/express": "~4.0.0",
    "@types/node": "~8.0.0",
    "@types/pg": "~7.1.0",
    "angular2-template-loader": "~0.6.0",
    "concurrently": "~3.5.0",
    "css-loader": "~0.28.0",
    "extract-text-webpack-plugin": "~3.0.0",
    "file-loader": "~1.1.0",
    "node-pg-migrate": "~2.14.0",
    "nodemon": "~1.12.0",
    "raw-loader": "~0.5.0",
    "style-loader": "~0.19.0",
    "ts-loader": "~3.1.0",
    "ts-node": "~3.3.0",
    "tslint": "~5.8.0",
    "typescript": "~2.6.0",
    "webpack": "~3.8.0",
    "webpack-dev-server": "~2.9.0"
  }
}
